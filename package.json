{
  "name": "csvtransformer",
  "version": "1.0.0",
  "description": "a small javascript library for easy csv transformation",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "cm": "cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eriknewland/csvTransformer.git"
  },
  "keywords": [
    "csv",
    "merge",
    "aggregate",
    "reduce",
    "data",
    "python",
    "pandas",
    "javascript",
    "typescript",
    "etl"
  ],
  "author": "Erik Newland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eriknewland/csvTransformer/issues"
  },
  "homepage": "https://github.com/eriknewland/csvTransformer#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.1",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "python-shell": "^5.0.0"
  },
  "files": [
    "lib/**/*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
